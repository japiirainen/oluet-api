{
  "name": "oluet-api",
  "version": "0.0.1",
  "description": "api for finding information about various diffenent beers",
  "main": "src/index.ts",
  "author": "Joona Piirainen",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.11.10",
    "@types/ramda": "^0.27.29",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "nodemon": "^2.0.5",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "build": "tsc",
    "prebuild": "yarn generate",
    "tscw": "tsc -w",
    "start": "yarn generate && node dist/index.js",
    "dev": "yarn generate && nodemon --exec ts-node --files src/index.ts",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "prisma:migrate": "prisma migrate save --experimental && prisma migrate up --experimental",
    "test": "jest --passWithNoTests",
    "postgres:start": "docker-compose up -d"
  },
  "dependencies": {
    "@nexus/schema": "^0.17.0",
    "@prisma/cli": "^2.10.1",
    "@prisma/client": "^2.10.1",
    "@types/node-fetch": "^2.5.7",
    "apollo-server-express": "^2.18.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fp-ts": "^2.8.4",
    "graphql": "^15.3.0",
    "graphql-middleware": "^4.0.2",
    "graphql-scalars": "^1.4.1",
    "jest": "^26.6.1",
    "nexus-plugin-prisma": "^0.23.0",
    "nodejs-file-downloader": "^3.0.1",
    "ramda": "^0.27.1",
    "xlsx": "^0.16.8"
  }
}
